<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="zz.utility">

    <uses-feature android:name="android.hardware.camera"/>

    <uses-permission android:name="android.permission.ACCESS_ASSISTED_GPS"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_GPS"/>
    <uses-permission android:name="android.permission.ACCESS_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.READ_CALL_LOG"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.RECEIVE_SMS"/>
    <uses-permission android:name="android.permission.READ_SMS"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.PROCESS_OUTGOING_CALLS"/>
    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.GET_ACCOUNTS"/>
    <uses-permission android:name="android.permission.GET_ACCOUNTS"/>
    <uses-permission android:name="android.permission.READ_PROFILE"/>

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="false"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NavOverlay.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>

            <meta-data
                android:name="android.app.shortcuts"
                android:resource="@xml/shortcuts"/>
        </activity>
        <activity
            android:name=".GPSActivity"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".QuoteActivity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".SensorsActivity"
            android:label="@string/title_activity_sensors"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".WiFiScannerActivity"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".QRCodeGeneratorActivity"
            android:label="@string/title_activity_qr_code_generator"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".InfoActivity"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".AndroidVersionsActivity"
            android:label="@string/title_activity_versions"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".ScanningActivity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar"/>

        <receiver
            android:name=".spy.SmsReceiver"
            android:enabled="true"
            android:exported="true"
            android:permission="android.permission.BROADCAST_SMS">
            <intent-filter android:priority="2147483647">
                <action android:name="android.provider.Telephony.SMS_RECEIVED"/>
            </intent-filter>
        </receiver>
        <receiver
            android:name=".spy.CallReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.PHONE_STATE"/>
            </intent-filter>
        </receiver>

        <activity
            android:name=".maps.MapsActivity"
            android:theme="@style/AppTheme.NavOverlay.NoActionBar">
            <intent-filter android:label="Open Link">
                <action android:name="android.intent.action.VIEW"/>

                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>

                <data
                    android:host="maps"
                    android:scheme="utility"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".poc.TestBottomNavigationActivity"
            android:label="@string/title_activity_test_bottom_navigation"
            android:parentActivityName=".poc.PocMenuActivity"/>
        <activity
            android:name=".poc.LoginActivity"
            android:label="@string/title_activity_login"
            android:parentActivityName=".poc.PocMenuActivity"
            android:theme="@style/AppTheme.Login"/>
        <activity
            android:name=".bot.BotActivity"
            android:parentActivityName=".poc.PocMenuActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".poc.POSActivity"
            android:parentActivityName=".poc.PocMenuActivity"
            android:screenOrientation="sensorLandscape"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".CameraScalesActivity"
            android:label="Camera Scales"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".scrum.ScrumPokerActivity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".browser.FileBrowserActivity"
            android:parentActivityName=".MainActivity"/>

        <provider
            android:name="android.support.v4.content.FileProvider"
            android:authorities="${applicationId}.provider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/provider_paths"/>
        </provider>

        <activity android:name=".browser.TextViewActivity"/>
        <activity
            android:name=".poc.WelcomeActivity"
            android:parentActivityName=".poc.PocMenuActivity"
            android:screenOrientation="sensorLandscape"/>

        <service
            android:name=".spy.MyNotificationListenerService"
            android:label="Notification Spy Listener"
            android:permission="android.permission.BIND_NOTIFICATION_LISTENER_SERVICE">
            <intent-filter>
                <action android:name="android.service.notification.NotificationListenerService"/>
            </intent-filter>
        </service>

        <activity
            android:name=".ListActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/title_activity_list"/>
        <activity
            android:name=".poc.DashboardActivity"
            android:parentActivityName=".poc.PocMenuActivity"/>
        <activity
            android:name=".browser.Gallery2Activity"
            android:label="@string/title_activity_gallery"
            android:parentActivityName=".browser.FileBrowserActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".poc.PocMenuActivity"
            android:label="POC Screens"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".poc.CardDemoActivity"
            android:label="POC Cards Layouts"
            android:parentActivityName=".poc.PocMenuActivity"/>
        <activity
            android:name=".ImageDownloadActivity"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".KnowledgeActivity"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="sensorLandscape"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".browser.VideoPlayerActivity"
            android:parentActivityName=".browser.FileBrowserActivity"
            android:screenOrientation="sensorLandscape"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".CarDockActivity"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="sensorLandscape"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".text.TextReceiveActivity">

            <!-- <intent-filter> -->
            <!-- <action android:name="android.intent.action.SEND" /> -->
            <!-- <category android:name="android.intent.category.DEFAULT" /> -->
            <!-- <data android:mimeType="image/*" /> -->
            <!-- </intent-filter> -->
            <intent-filter>
                <action android:name="android.intent.action.SEND"/>

                <category android:name="android.intent.category.DEFAULT"/>

                <data android:mimeType="text/plain"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".DailyComicActivity"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".QuoteApiActivity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".TouchScreenActivity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
    </application>

</manifest>